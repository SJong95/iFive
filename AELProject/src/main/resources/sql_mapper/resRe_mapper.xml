<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="resRe">

<!-- @@@@@@@@@ -->
	<insert id="resReportAccept" parameterType="resRe">
		INSERT INTO RESREPORT VALUES(#{RBNUM}, #{RNAME}, #{ID}, #{RDVI}, #{RCONTENT})
	</insert>
		<select id="resReportList" resultType="resRe">
		SELECT * FROM RESREPORT
	</select>

</mapper>